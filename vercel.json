{
  "buildCommand": "rm -rf api pages/api .vercel/api 2>/dev/null || true; pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install -g pnpm@latest && pnpm install --no-frozen-lockfile",
  "devCommand": "pnpm run dev",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/samples.json",
      "destination": "/samples.json"
    },
    {
      "source": "/examples-in-load.json",
      "destination": "/examples-in-load.json"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}


























































